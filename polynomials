%4.1
p=[1 -2  6 -10 16];
x0=4;H=[1 -x0];
[q,r]=deconv(p,H);
[q2,r2]=deconv(q,H);
disp(r2(4));
disp(polyval(polyder(p),x0));
%%
%4.2
p = [1 (1+2*1i) 0 -(1+3*1i) 0 7];
x0=-2-1i;H=[1 -x0];
[q,r]=deconv(p,H);
[q2,r2]=deconv(q,H);
disp(r2(5));
disp(polyval(polyder(p),x0));
%%
%5
syms n;syms x;
p=sym2poly(subs(x^(2*n)-n*x^(n+1)+n*x^(n-1)-1,n,2));
disp(roots(p));
x=linspace(-1,1,100);
plot(x,polyval(p,x));
grid on
